{
  "name": "@polywrap/ipfs-http-client-wasm",
  "description": "A Polywrap Wasm wrapper that implements an IPFS Client",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn codegen && npx polywrap build -s image",
    "codegen": "npx polywrap codegen",
    "deploy": "npx polywrap deploy -o deployment.json",
    "test": "yarn test:unit && yarn test:e2e",
    "test:ci": "yarn test:unit && yarn test:e2e",
    "test:unit": "asp --verbose",
    "test:e2e": "yarn test:e2e:codegen && jest --passWithNoTests --runInBand --verbose",
    "test:e2e:codegen": "npx polywrap codegen -m ./src/__tests__/e2e/types/polywrap.app.yaml -g ./src/__tests__/e2e/types/wrap"
  },
  "dependencies": {
    "@polywrap/wasm-as": "~0.10.0",
    "as-base64": "0.2.0"
  },
  "devDependencies": {
    "@as-pect/cli": "6.2.4",
    "@polywrap/cli-js": "0.11.0-pre.4",
    "@polywrap/client-config-builder-js": "0.12.0-pre.1",
    "@polywrap/client-js": "0.12.0-pre.1",
    "@polywrap/http-plugin-js": "~0.10.0",
    "@types/jest": "26.0.8",
    "@types/node": "16.11.64",
    "assemblyscript": "0.19.23",
    "axios": "0.21.4",
    "jest": "26.6.3",
    "polywrap": "0.11.0-pre.4",
    "ts-jest": "26.5.4",
    "ts-node": "10.9.1",
    "typescript": "4.6.4"
  }
}
